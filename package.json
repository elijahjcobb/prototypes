{
  "name": "@elijahjcobb/prototypes",
  "version": "0.0.5",
  "description": "A collection of helpful classes to manage various real world things in TypeScript.",
  "keywords": [
    "color",
    "address",
    "byte",
    "size",
    "file",
    "filesystem",
    "date",
    "time",
    "interval",
    "wait",
    "regex",
    "mime"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && /usr/local/bin/tsc -p tsconfig.json",
    "watch": "tsc -w tsconfig.json",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahjcobb/prototypes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Elijah Cobb <elijah@elijahcobb.com> (https://elijahcobb.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elijahjcobb/prototypes/issues"
  },
  "homepage": "https://github.com/elijahjcobb/prototypes#readme",
  "dependencies": {
    "@elijahjcobb/collections": "latest",
    "@elijahjcobb/encryption": "latest",
    "@elijahjcobb/error": "latest",
    "@types/node": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "typescript": "*",
    "tslint": "*"
  },
  "files": [
    "dist"
  ]
